                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @sis_id int
	set @sis_id =  (select 
						max(cast(SUBSTRING(sis_user_id,CHARINDEX('G',sis_user_id)+1,len(sis_user_id)) as int))
					from pseudonym_dim
					where left(sis_user_id, charindex('G',sis_user_id)) like 'pG')
--set @sis_id = 4859
select distinct
	u1.First_Name pFirstName
	,u1.Last_Name pLastName
	,u1.email pEmail
	,u1.user_id pUserId
	,jc.StudentIndex
	,jc.StudentFirstName
	,jc.StudentLastName
from
(
	select distinct
			concat('pG',@sis_id + ROW_NUMBER() over (order by s1.parentemail)) user_id
			,s1.ParentEmail as login_id
			,'1' as authentication_provider_id
			,LEFT(NEWID(), 8 ) password
			,s1.ParentFirstName First_Name
			,s1.ParentLastName Last_Name
			,'' as shortname
			,s1.ParentEmail email
			,'active' as status
	from
		(select	
			ParentEmail
			,ParentFirstName
			,ParentLastName
			,row_number() over (partition by parentEmail order by ParentEmail) as rn
		from jcampus.dbo.JCampusStudentParent) s1
	where 
	rn = 1
	and s1.ParentEmail not in
		(select unique_name from pseudonym_dim)

	union

	select distinct
		p.sis_user_id as user_id
		,s1.ParentEmail as login_id
		,'1' as authentication_provider_id
		,LEFT(NEWID(), 8 ) password
		,s1.ParentFirstName First_Name
		,s1.ParentLastName Last_Name
		,'' as short_name
		,s1.ParentEmail email
		,'active' as status
	from
		(select	
			ParentEmail
			,ParentFirstName
			,ParentLastName
			,row_number() over (partition by parentEmail order by ParentEmail) as rn
		from jcampus.dbo.JCampusStudentParent) s1
		inner join pseudonym_dim p on p.unique_name = s1.parentemail
	where 
	rn = 1
	and s1.ParentEmail not in
		(select unique_name from pseudonym_dim where workflow_state = 'active')
                   and s1.ParentEmail not in ('rbaudry@abt-law.com')
) as u1
inner join jcampus.dbo.JCampusStudentParent jc on jc.ParentEmail = u1.login_id
order by StudentIndex</property>
