            <component
              refId="Package\users_observers csv\OLE DB Source"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--Set a variable for the largest parent SIS ID currently in the system
declare @sis_id int
	set @sis_id =  (select 
						max(cast(SUBSTRING(sis_user_id,CHARINDEX('G',sis_user_id)+1,len(sis_user_id)) as int))
					from pseudonym_dim
					where left(sis_user_id, charindex('G',sis_user_id)) like 'pG')


select distinct
	u1.user_id as observer_id
	,jc.StudentIndex as student_id
	,'active' as status
from
(
	--Find student / parent matches where parent email addresses are in Jcampus but not in Canvas
	select distinct
			concat('pG',@sis_id + ROW_NUMBER() over (order by s1.parentemail)) user_id
			,s1.ParentEmail as login_id
			,'1' as authentication_provider_id
			,LEFT(NEWID(), 8 ) password
			,s1.ParentFirstName First_Name
			,s1.ParentLastName Last_Name
			,'' as shortname
			,s1.ParentEmail email
			,'active' as status
	from
		(select	
			ParentEmail
			,ParentFirstName
			,ParentLastName
			,row_number() over (partition by parentEmail order by ParentEmail) as rn
		from jcampus.dbo.JCampusStudentParent) s1
	where 
	rn = 1
	and s1.ParentEmail not in
		(select unique_name from pseudonym_dim)

	union

	--Find student / parent combinations where parent email address is in canvas, but canvas account is not active	
	select distinct
		p.sis_user_id as user_id
		,s1.ParentEmail as login_id
		,'1' as authentication_provider_id
		,LEFT(NEWID(), 8 ) password
		,s1.ParentFirstName First_Name
		,s1.ParentLastName Last_Name
		,'' as short_name
		,s1.ParentEmail email
		,'active' as status
	from
		(select	
			ParentEmail
			,ParentFirstName
			,ParentLastName
			,row_number() over (partition by parentEmail order by ParentEmail) as rn
		from jcampus.dbo.JCampusStudentParent) s1
		inner join pseudonym_dim p on p.unique_name = s1.parentemail
	where 
	rn = 1
	and s1.ParentEmail not in
		(select unique_name from pseudonym_dim where workflow_state = 'active')

	union

	--Find student / parent combinations for all students that have started in the past month. 
	--Catches any new students associated with old parent accounts
	select distinct
		p.sis_user_id as user_id
		,s1.ParentEmail as login_id
		,'1' as authentication_provider_id
		,LEFT(NEWID(), 8 ) password
		,s1.ParentFirstName First_Name
		,s1.ParentLastName Last_Name
		,'new account' as short_name
		,s1.ParentEmail email
		,'active' as status
	from
		(select	
			ParentEmail
			,ParentFirstName
			,ParentLastName
			,StudentEntryDate
			,row_number() over (partition by parentEmail order by ParentEmail) as rn
